<?xml version="0.1"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>johnlzeller@gmail.com</em:id>
        <em:name>TeamCyclops</em:name>
        <em:version>0.1</em:version>
        <em:optionsURL>chrome://TeamCyclops/content/options.xul</em:optionsURL>
		  <em:iconURL>chrome://TeamCyclops/skin/logo.png</em:iconURL>
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>0.1</em:minVersion>
                <em:maxVersion>1.0.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:creator>Jonathan Fessler, Elliot Gladden, Patrick Jarrold, Zhi Yan, John Zeller and Yi Zhao</em:creator>
        <em:description>A browser plugin that allows the user to have a master password that controls randomized and secure passwords for a variety of accounts on websites throughout the internet. This localizes the process and securing of password information to the users computer, while keeping the login experience as seamless as possible for the user. No more annoying copy/pasting!</em:description>
        <em:homepageURL>https://github.com/pjarrold/TeamCyclops</em:homepageURL>
		  <em:updateURL>https://github.com/pjarrold/TeamCyclops/update.rdf</em:updateURL>
    </Description>

</RDF>
